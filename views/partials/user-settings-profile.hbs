<!-- Form -->
<form class="mb-6">

    <div class="row">
        <div class="col-12">

            <!-- First name -->
            <div class="form-group">

                <!-- Label -->
                <label>
                    Headline
                </label>

                <small class="form-text text-muted">
                    Tell us a little about yourself, e.g., Software Engineer at ACME Corp.
                </small>

                <!-- Input -->
                <input id="user-profile-input-headline" maxlength="120" value="{{data.profile.headline}}" placeholder="Short sentence about yourself" type="text" class="form-control">

            </div>

        </div>
        <div class="col-12">

            <!-- First name -->
            <div class="form-group">

                <!-- Label -->
                <label>
                    Username
                </label>

                <small class="form-text text-muted">
                    A short unique handle for your profile
                </small>

                <!-- Input -->
                <input id="user-profile-input-username" maxlength="60" pattern="[A-Za-z0-9]" value="{{data.profile.username}}" placeholder="Only lowercase, without spaces, username" type="text" class="form-control">

            </div>

        </div>
        <div class="col-12 col-md-6">

            <!-- First name -->
            <div class="form-group">

                <!-- Label -->
                <label>
                    Full name
                </label>

                <small class="form-text text-muted">
                    This is the name we'll use to address you in our communications
                </small>

                <!-- Input -->
                <input id="user-profile-input-full_name" value="{{data.profile.full_name}}" type="text" class="form-control">

            </div>

        </div>
        <div class="col-12 col-md-6">

            <!-- Email address -->
            <div class="form-group">

                <!-- Label -->
                <label class="mb-1">
                    Email address
                </label>

                <!-- Form text -->
                <small class="form-text text-muted">
                    Email update is currently disabled, please contact support if you would like to update your email
                </small>

                <!-- Input -->
                <input id="user-profile-input-primary_email" style="cursor: not-allowed;" value="{{data.profile.primary_email}}" disabled placeholder="Email update is currently disabled" type="email" class="form-control">

            </div>

        </div>
        <div class="col-12 col-md-6">

            <!-- Phone -->
            <div class="form-group">

                <!-- Label -->
                <label>
                    Twitter Username
                </label>

                <small class="form-text text-muted">
                    Your Twitter handle, e.g., BillGates
                </small>

                <!-- Input -->
                <input id="user-profile-input-twitter" type="text" class="form-control mb-3" placeholder="Your Twitter username">

            </div>

        </div>
        <div class="col-12 col-md-6">

            <!-- Phone -->
            <div class="form-group">

                <!-- Label -->
                <label>
                    LinkedIn Username
                </label>

                <small class="form-text text-muted">
                    Your LinkedIn handle, e.g., williamhgates
                </small>

                <!-- Input -->
                <input id="user-profile-input-linkedin" type="text" class="form-control mb-3" placeholder="Your LinkedIn username">

            </div>

        </div>

        <div class="col-12">
            <button type="submit" class="btn btn-primary">
                Update Profile
            </button>
        </div>

    </div> <!-- / .row -->

