<link rel="" href="/learn-en/assets/css/theme-dark.min.css" data-toggle="theme" data-theme-mode="dark">
<link rel="" href="/learn-en/assets/css/theme.min.css" data-toggle="theme" data-theme-mode="light">

<style>
    body { display: none; }
</style>

<script>
    var themeMode = (localStorage.getItem('themeMode')) ? localStorage.getItem('themeMode') : 'light';
    var themeFile = document.querySelector('[data-toggle="theme"][data-theme-mode="' + themeMode + '"]');

    // Enable stylesheet
    themeFile.rel = 'stylesheet';

    // Enable body content
    themeFile.addEventListener('load', function() {
        document.body.style.display = 'block';
    });
</script>

<style>
    .iti-flag {background-image: url("/learn-en/assets/libs/intl-tel-input/v15.0.0/img/flags.png");}

    @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
    .iti-flag {background-image: url("/learn-en/assets/libs/intl-tel-input/v15.0.0/img/flags@2x.png");}
    }
    .intl-tel-input { width: 100%; }
</style>

