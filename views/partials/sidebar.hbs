<style>
    .cc-nav-item {

    }
</style>
<div class="container-fluid">

    <!-- Toggler -->
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#sidebarCollapse" aria-controls="sidebarCollapse" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    <!-- Brand -->
    <a class="navbar-brand" href="/learn-{{route.locale}}/dashboard">
        <img src="/learn-en/assets/img/logo-exlskills.svg" class="navbar-brand-img
  mx-auto" alt="...">
    </a>

    <!-- User (xs) -->
    <div class="navbar-user d-md-none">
        {{#if user.is_demo}}
            <a href="{{config.authAPIBaseURL}}/auth/keycloak?redirect={{encodeURI route.url}}" class="btn btn-success">Login / Sign Up</a>
        {{else}}
            <!-- Dropdown -->
            <div class="dropdown">

                <!-- Toggle -->
                <a href="#!" id="sidebarIcon" class="dropdown-toggle" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <div class="avatar avatar-sm avatar-online">
                        <img src="{{user.avatar_url}}" class="avatar-img rounded-circle" alt="...">
                    </div>
                </a>

                <!-- Menu -->
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="sidebarIcon">
                    <a href="{{{config.accountsURL}}}" class="dropdown-item">Profile</a>
                    <!-- TODO <a href="settings.html" class="dropdown-item">Settings</a>-->
                    <hr class="dropdown-divider">
                    <a href="#!" class="dropdown-item user-logout-btn">Logout</a>
                </div>

            </div>
        {{/if}}
    </div>

    <!-- Collapse -->
    <div class="collapse navbar-collapse" id="sidebarCollapse">

        <!-- Form -->
        <form class="mt-4 mb-3 d-md-none">
            <div class="input-group input-group-rounded input-group-merge">
                <input type="search" class="form-control form-control-rounded form-control-prepended" placeholder="Search" aria-label="Search">
                <div class="input-group-prepend">
                    <div class="input-group-text">
                        <span class="fe fe-search"></span>
                    </div>
                </div>
            </div>
        </form>

        <!-- Navigation -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link spf-link {{#eq contentTmpl "dashboard"}}active{{/eq}}" href="/learn-{{route.locale}}/dashboard">
                    <i class="fe fe-home"></i> Dashboard
                </a>
            </li>
            <!--<li class="nav-item">-->
                <!--<a class="nav-link spf-link {{#eq contentTmpl "digital_diplomas"}}active{{/eq}}" href="/learn-{{route.locale}}/digital-diplomas">-->
                    <!--<i class="fe fe-star"></i> Digital Diplomas-->
                <!--</a>-->
            <!--</li>-->
            <li class="nav-item">
                <a class="nav-link spf-link {{#eq contentTmpl "projects"}}active{{/eq}}" href="/learn-{{route.locale}}/projects">
                    <i class="fe fe-package"></i> Guided Projects
                </a>
            </li>
            <!--<li class="nav-item">-->
                <!--<a class="nav-link spf-link {{#eq contentTmpl "instructors"}}active{{/eq}}" href="/learn-{{route.locale}}/instructors">-->
                    <!--<i class="fe fe-users"></i> Live Instructors-->
                <!--</a>-->
            <!--</li>-->
            <li class="nav-item">
                <a class="nav-link spf-link {{#eq contentTmpl "courses"}}active{{/eq}}" href="/learn-{{route.locale}}/courses">
                    <i class="fe fe-zap"></i> Open Courses
                </a>
            </li>
            <li class="nav-item d-md-none">
                <a class="nav-link" href="#!modalManageCoins">
                    <span class="fe fe-box"></span> ${{user.coins}} Coins
                </a>
            </li>
        </ul>

        <!-- Course Sidebar -->
        {{#if data.course}}
            <!-- Divider -->
            <hr class="my-3">

            <!-- Heading -->
            <h6 class="navbar-heading text-muted">
                {{data.course.meta.title}}
            </h6>

            <!-- Navigation -->
            <ul class="navbar-nav mb-md-3">
                {{#if data.course.meta.instructor_timekit.intervals}}
                    <li class="nav-item">
                        <a class="text-primary nav-link spf-link {{#eq contentTmpl "course_help"}}active{{/eq}}" href="/learn-{{route.locale}}/courses/{{data.course.meta.url_id}}/help">
                            <i class="fe fe-help-circle"></i> Get Help! ✋
                        </a>
                    </li>
                {{/if}}
                <li class="cc-nav-item nav-item">
                    <a class="nav-link spf-link {{#eq contentTmpl "course_content"}}active{{/eq}}" href="/learn-{{route.locale}}/courses/{{data.course.meta.url_id}}/content">
                        <i class="fe fe-book"></i> Course
                    </a>
                    <ul class="nav nav-sm flex-column">
                        <li class="nav-item">
                            <a class="nav-link spf-link {{#eq contentTmpl "course_index"}}active{{/eq}}" href="/learn-{{route.locale}}/courses/{{data.course.meta.url_id}}">
                                Overview <span style="font-size: 12px;" class="ml-2 fe {{#if data.course.isEnrolled}}fe-check text-success{{/if}}"></span>
                            </a>
                        </li>
                        {{#each data.course.units}}
                            <li class="nav-item">
                                <a href="#sb-course-unit-{{url_id}}" class="nav-link" data-toggle="collapse" role="button" aria-expanded="{{#eq url_id ../route.params.unitId}}true{{/eq}}" aria-controls="sb-course-unit-{{url_id}}">
                                    {{title}} <span style="font-size: 12px;" class="ml-2 fe {{#gt ema 80}}fe-check text-success{{else}}{{#gt ema 0}}fe-refresh-ccw text-warning{{/gt}}{{/gt}}"></span>
                                </a>
                                <div class="collapse {{#eq url_id ../route.params.unitId}}show{{/eq}}" id="sb-course-unit-{{url_id}}">
                                    <ul class="nav nav-sm flex-column">
                                        {{#each sections_list}}
                                            <li class="nav-item">
                                                <h6 class="nav-link navbar-heading text-muted m-0">
                                                    <a href="/learn-{{../../route.locale}}/courses/{{../../data.course.meta.url_id}}/{{../url_id}}/{{url_id}}/{{first_card_url_id}} " class="spf-link">
                                                    {{title}}
                                                    </a>
                                                </h6>
                                                {{#each cards_list}}
                                                    <a href="/learn-{{../../../route.locale}}/courses/{{../../../data.course.meta.url_id}}/{{../../url_id}}/{{../url_id}}/{{url_id}} " class="spf-link nav-link {{#eq url_id ../../../route.params.cardId}}active{{/eq}}">
                                                        <span class="{{#gt ema 80}}text-success{{else}}{{#gt ema 0}}text-warning{{/gt}}{{/gt}}">●</span> <div class="ml-2">{{title}}</div>
                                                    </a>
                                                {{/each}}
                                            </li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </li>
                        {{/each}}
                    </ul>
                </li>
                <!-- Course Progress Removed until/if exams are implemented, otherwise useless
                <li class="nav-item">
                    <a class="nav-link spf-link {{#eq contentTmpl "course_progress"}}active{{/eq}}" href="/learn-{{route.locale}}/courses/{{data.course.meta.url_id}}/progress">
                        <i class="fe fe-trending-up"></i> Progress
                    </a>
                </li>
                -->
                <li class="nav-item">
                    <a class="nav-link spf-link {{#eq contentTmpl "course_certificate"}}active{{/eq}}" href="/learn-{{route.locale}}/courses/{{data.course.meta.url_id}}/certificate">
                        <i class="fe fe-award"></i> Certificate
                    </a>
                </li>
                {{#if data.course.meta.has_live_mode}}
                <li class="nav-item">
                    <a class="nav-link spf-link {{#eq contentTmpl "course_live"}}active{{/eq}}" href="/learn-{{route.locale}}/courses/{{data.course.meta.url_id}}/live">
                        <i class="fe fe-radio"></i> Live Class
                    </a>
                </li>
                {{/if}}
            </ul>
        {{/if}}

        <!-- Divider -->
        <hr class="my-3">

        <!-- Heading -->
        <h6 class="navbar-heading text-muted">
            Configuration
        </h6>

        <!-- Navigation -->
        <ul class="navbar-nav mb-md-3">
            <li class="nav-item">
                <div class="nav-link text-muted">
                    <i class="fe fe-sun"></i> Night mode
                    <div class="custom-control custom-checkbox-toggle ml-auto">
                        <input type="checkbox" class="custom-control-input" id="theme-mode-toggle">
                        <label class="custom-control-label" for="theme-mode-toggle"></label>
                    </div>
                </div>
            </li>
        </ul>

    </div> <!-- / .navbar-collapse -->

</div> <!-- / .container-fluid -->
